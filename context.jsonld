{
  "@context": [
    "https://cdn.jsdelivr.net/gh/valueflows/valueflows@master/context.jsonld",
    {
      "Agent": "vf:Agent",
      "Person": "vf:Person",
      "Group": "vf:Group"
    }
  ],
  "@graph": [
    {
        "@id": "Agent",
        "@type": "owl:Class",
        "rdfs:label": {
            "en": "Agent"
        }
    },
    {
        "@id": "Person",
        "@type": "owl:Class",
        "rdfs:label": {
            "en": "Person"
        },
        "rdfs:subClassOf": {
            "@id": "Agent"
        }
    },
    {
        "@id": "Group",
        "@type": "owl:Class",
        "rdfs:label": {
            "en": "Group"
        },
        "rdfs:subClassOf": {
            "@id": "Agent"
        }
    }
  ]
}
