{
  "@context": [
    "https://rawgit.com/valueflows/valueflows/master/context.jsonld",
    {
      "AgentType": "vf:AgentType",
      "Agent": "vf:Agent",
      "Person": "vf:Person",
      "Group": "vf:Group"
    }
  ],
  "@graph": [
    {
      "@id": "AgentType",
      "@type": "owl:Class",
      "rdfs:label": {
        "en": "Agent Type"
      }
    },
    {
        "@id": "Agent",
        "@type": "AgentType",
        "rdfs:label": {
            "en": "Agent"
        }
    },
    {
        "@id": "Person",
        "@type": "AgentType",
        "rdfs:label": {
            "en": "Person"
        },
        "rdfs:subClassOf": {
            "@id": "Agent"
        }
    },
    {
        "@id": "Group",
        "@type": "AgentType",
        "rdfs:label": {
            "en": "Group"
        },
        "rdfs:subClassOf": {
            "@id": "Agent"
        }
    }
  ]
}
